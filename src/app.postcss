.page-wraper.header-fixed {
	&:before {
		content: "";
		height: var(--headheight);
		width: 100%;
		display: table;
		z-index: -1;
		position: relative;
	}
	.header {
		position: fixed;
		top: 0;
		z-index: 999;
		left: 0;
		background-color: var(--primary-light);
		backdrop-filter: blur(10px);
		transition: all 0.5s;
		width: 100%;
	}
}
.sticky-header {
	position: fixed;
	top: 0;
	z-index: 99999;
	left: 0;
	background-color: #FFFBF6;
	transition: all 0.5s;
	width: 100%;
}
.header {
	min-height: var(--headheight);
	z-index: 2;
	border-bottom: 1px solid var(--border-color-2);
	.back-btn {
		display: flex;
		line-height: 1;
		font-size: 20px;
		width: 35px;
		height: 35px;
		min-width: 35px;
		align-items: center;
		justify-content: center;
		margin-right: 15px;
		color: #2f2f2f;
	}
	.menu-toggler {
		font-size: 20px;
		color: var(--title);
		display: flex;
		align-items: center;
		justify-content: center;
		i {
			line-height: 1;
		}
	}
	.container {
		padding-top: 0;
		padding-bottom: 0;
	}
	.main-bar {
		display: flex;
		height: 100%;
		min-height: var(--headheight);
		align-items: center;
		.left-content {
			min-width: 30px;
			height: 100%;
			display: flex;
			align-items: center;
		}
		.mid-content {
			flex: 1;
			text-align: center;
		}
		.right-content {
			.form-check.checkmark {
				padding: 0;
				.form-check-label {
					width: 20px;
					height: 20px;
					cursor: pointer;
					border-radius: 0;
					text-align: center;
					border: 0;
					line-height: 0;
				}
				.form-check-input {
					display: none;
					&:checked {
						+ {
							.form-check-label {
								background-color: transparent;
								svg {
									path {
										fill: var(--primary);
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
.header.transparent {
	position: absolute;
	width: 100%;
	top: 0;
	background-color: transparent !important;
	border-bottom: 1px solid var(--border-color-2) !important;
	border: 0;
	left: 0;
	z-index: 15;
	.left-content {
		.back-btn {
			color: #2f2f2f;
		}
		.menu-toggler {
			color: #000;
			svg {
				path {
					fill: #bfc9da;
				}
			}
		}
	}
	.main-bar {
		.back-btn {
			svg {
				path {
					fill: #fff;
				}
			}
		}
		.menu-toggler {
			color: #fff;
		}
		&:not(.sticky-header) {
			.form-check.checkmark {
				padding: 0;
				.form-check-label {
					width: 20px;
					height: 20px;
					cursor: pointer;
					border-radius: 0;
					text-align: center;
					border: 0;
					line-height: 0;
					svg {
						path {
							stroke: #fff;
						}
