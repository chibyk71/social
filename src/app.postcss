	flex-direction: column;
	.r-btn {
		color: #fff;
		text-align: center;
		font-size: 25px;
		padding-bottom: 12px;
		svg {
			width: 24px;
			height: 24px;
			path {
				fill: #fff;
			}
		}
		span {
			display: block;
			font-size: 14px;
		}
	}
}
.mySwiper {
	height: 100vh;
	overflow: hidden;
}
.features-list {
	display: flex;
	align-items: center;
	justify-content: space-around;
	li {
		text-align: center;
		.dz-icon {
			width: 60px;
			height: 60px;
			border: 1px solid #2F2A2A;
			border-radius: 50%;
