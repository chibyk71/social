.reel-canvas {
	width: 100%;
	max-width: 500px;
	margin: 0 auto;
}
.get-started {
	text-align: center;
	flex: 1;
	width: 100%;
	.swiper-wrapper {
		margin-bottom: 0;
	}
	.dz-title {
		margin-bottom: 12px;
		font-size: 24px;
	}
	p {
		margin-bottom: 0;
		margin-top: 0;
	}
}
.swiper-btn {
	display: flex;
	width: 100%;
	align-items: center;
	justify-content: space-between;
	padding: 0 1.5rem;
}
.swiper-button-prev {
	i {
		transform: rotateY(180deg);
		line-height: .5;
	}
	position: unset;
	height: 3rem;
	opacity: 1;
	width: 3rem;
	margin: 0;
	font-size: 1.3rem;
	display: flex;
	align-items: center;
	justify-content: center;
	border-radius: 3rem;
	backgroun: transparent;
	-webkit-transition: all 0.5s;
	-ms-transition: all 0.5s;
	transition: all 0.5s;
	color: #767676;
	border: 1px solid #CBCBCB;
	&:after {
		content: none;
	}
	&:hover {
		background: var(--primary);
		color: #fff;
	}
}
.swiper-button-next {
	position: unset;
	height: 3rem;
	opacity: 1;
	width: 3rem;
	margin: 0;
	font-size: 1.3rem;
	display: flex;
	align-items: center;
	justify-content: center;
	border-radius: 3rem;
	backgroun: transparent;
	-webkit-transition: all 0.5s;
	-ms-transition: all 0.5s;
	transition: all 0.5s;
	color: #767676;
	border: 1px solid #CBCBCB;
	i {
		line-height: .5;
	}
	&:after {
		content: none;
	}
	&:hover {
		background: var(--primary);
		color: #fff;
	}
}
.splide__pagination.style-1 {
	bottom: 0 !important;
	opacity: 1;
	position: unset;
	padding: 10px 0;
	z-index: 1;
	.splide__pagination__page {
		margin: 0 4px;
		width: 8px;
		height: 8px;
		border-radius: 50%;
		background: #C4C4C4;
		opacity: 1;
		-webkit-transition: all 0.5s;
		-ms-transition: all 0.5s;
		transition: all 0.5s;
		-webkit-transition: all 0.5s;
		-ms-transition: all 0.5s;
		transition: all 0.5s;
	}
	.splide__pagination__page.is-active {
		background: var(--primary);
		transform: scale(1.5);
	}
}
.swiper-pagination.style-2 {
	bottom: 0 !important;
	opacity: 1;
	position: absolute;
	bottom: 0;
	left: 50%;
	transform: translateX(-50%);
	padding: 10px 0;
	z-index: 1;
	.swiper-pagination-bullet {
		margin: 0 4px;
		width: 10px;
		height: 10px;
		border-radius: 5px;
		background: #fff;
		opacity: 1;
		-webkit-transition: all 0.5s;
		-ms-transition: all 0.5s;
		transition: all 0.5s;
		-webkit-transition: all 0.5s;
		-ms-transition: all 0.5s;
		transition: all 0.5s;
	}
	.swiper-pagination-bullet-active {
		background: var(--primary);
	}
}
.started-swiper-box {
	max-width: 1024px;
	margin-left: auto;
	margin-right: auto;
	width: 100%;
	display: flex;
	padding: 0;
	flex-direction: column;
	height: calc(100vh - 80px);
	.swiper-slide {
		height: 100%;
		position: relative;
	}
	.dz-media {
		margin: auto auto 40px;
		img {
			max-width: 250px;
		}
	}
	.slide-info {
		height: 100%;
		display: flex;
		flex-direction: column;
		justify-content: space-between;
		box-sizing: border-box;
		padding-bottom: 10px;
		background-repeat: no-repeat;
		background-position: top left;
		background-size: 100%;
		z-index: 1;
		position: relative;
	}
	.view-logo {
		margin: auto;
		+ {
			.slide-content {
				margin-top: 0;
			}
		}
	}
	.slide-content {
		z-index: 1;
		position: relative;
		max-width: 320px;
		margin: auto auto;
		text-align: center;
		padding: 15px 15px 0;
	}
	.bg-icon {
		position: absolute;
		left: 0;
		top: 50%;
		opacity: 0.1;
		transform: translateY(-50%);
	}
}
.page-onboading {
	.swiper-btn {
		margin-top: 0;
	}
	+ {
		.footer {
			background-color: var(--bg-white);
		}
	}
}
.main-content {
	display: flex;
	height: 100%;
	align-items: center;
	.left-content {
		width: 30px;
		height: 100%;
		display: flex;
		align-items: center;
	}
	.mid-content {
		flex: 1;
	}
}
.map {
	iframe {
		width: 100%;
		height: 100%;
		position: fixed;
	}
}
.detail-box {
	display: flex;
	align-items: center;
	justify-content: space-between;
	min-width: 320px;
	width: 100%;
	height: 115px;
	padding: 0 20px;
	background-color: var(--dark);
	border-radius: var(--border-radius-base);
	position: fixed;
	bottom: 15px;
	left: 15px;
	width: calc(100% - 30px);
	.handler {
		position: absolute;
		width: 45px;
		height: 6px;
		background-color: rgba(255, 255, 255, 0.6);
		border-radius: var(--border-radius-base);
		top: 10px;
		left: 50%;
		transform: translateX(-50%);
	}
	.media {
		img {
			border-radius: var(--border-radius-base);
		}
	}
	.social {
		a {
			width: 50px;
			min-width: 50px;
			height: 50px;
			text-align: center;
			line-height: 50px;
			color: #fff;
			display: inline-block;
			font-size: 18px;
		}
	}
}
.location-area {
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin: 15px 0px 28px 0;
	.location-mark {
		width: 40px;
		min-width: 40px;
		height: 40px;
		line-height: 37px;
		text-align: center;
		background-color: var(--bg-white);
		border: 2px solid var(--border-color);
		border-radius: var(--border-radius-base);
	}
}
.short-map {
	overflow: hidden;
	display: inline-block;
	box-shadow: 0 3px 12px -3px #dfd5d5;
	position: relative;
	border-radius: var(--border-radius-base);
	.zone {
		position: absolute;
		background: rgba(255, 255, 255, 0.56);
		border-radius: var(--border-radius-base);
		padding: 10px;
		backdrop-filter: blur(5px);
		text-align: center;
		top: 20px;
		left: 50%;
		transform: translateX(-50%);
		box-shadow: 0px 4px 23px 0px rgba(0, 0, 0, 0.25);
		.time {
			font-size: 18px;
			font-weight: 600;
			color: var(--title);
			line-height: 1;
		}
	}
	img {
		width: 100%;
		border-radius: var(--border-radius-base);
	}
}
.dz-timeline.style-2 {
	border: 0;
	margin: 15px 0;
	padding: 0;
	.timeline-item {
		padding: 10px 20px 10px 60px;
		border-radius: var(--border-radius-base);
		position: relative;
		&:before {
			content: "";
			position: absolute;
			height: 100%;
			border: 0;
			left: 30px;
			top: 50%;
			border-left: 2px solid var(--border-color);
			border-style: dashed;
			z-index: -1;
		}
		&:after {
			left: 24px;
			background: var(--bg-white);
			border: 2px solid var(--border-color);
			top: 50%;
			transform: translateY(-50%);
		}
	}
	.timeline-item.active {
		background-color: var(--primary);
		.timeline-tilte {
			color: #fff;
		}
		.timeline-date {
			color: #fff;
		}
		&:before {
			border-style: solid;
			border-color: var(--primary);
		}
		&:after {
			content: "\f00c";
			font-weight: 700;
			font-family: 'Font Awesome 6 Free';
			color: var(--primary);
			text-align: center;
			height: 28px;
			width: 28px;
			border-radius: 50%;
			left: 16px;
			box-shadow: 0 0 0 5px rgba(255, 255, 255, 0.2);
		}
	}
	.timeline-item.process {
		&:before {
			border-style: solid;
			border-color: var(--primary);
		}
		&:after {
			background-color: var(--primary);
			border-color: #fff;
		}
	}
	.timeline-tilte {
		margin-top: 0;
		margin-bottom: 3px;
	}
	.timeline-date {
		color: rgba(0, 0, 0, 0.7);
		font-size: 12px;
	}
}
#preloader {
	background-color: #fff;
	padding: 0;
	margin: 0;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 9999999;
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	.spinner {
		width: 3rem;
		height: 3rem;
		border: 5px solid #e1ded9;
		background: transparent;
		border-top-color: #8e8e8f;
		border-radius: 50%;
		animation: loading 1s linear infinite;
	}
}
@media (prefers-reduced-motion: reduce) {
	.accordion-header-indicator {
		&::before {
			transition: none;
		}
	}
}
@media only screen and (max-width: 87.5rem) {
	.dropdown-menu {
		.dropdown-item {
			padding: 0.375rem 1rem;
			font-size: 0.875rem;
		}
	}
}
@media only screen and (max-width: 35.9375rem) {
	.modal-header {
		padding: 1rem 1rem 1rem;
	}
	.modal-body {
		padding: 1rem;
	}
	.modal-footer {
		padding: 1rem;
	}
}
@media only screen and (min-width: 420px) {
	.welcome-area {
		.join-area {
			&:after {
				background-size: contain;
			}
		}
	}
}

